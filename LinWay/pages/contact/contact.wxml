<!--pages/contact/contact.wxml-->
<image  class='head_image' src="{{contact_image}}" mode='widthFix'></image>

<view class='container' wx:for="{{list}}" wx:key="key">
  <text class='item_key'>{{item.key}}</text>
  <text class='item_value' selectable="{{selectable}}">{{item.value}}</text>
</view>

<view class='foot'>
  <text class='foot_text'>请将网址复制到浏览器</text>
</view>